import Native;

export class IO:

    export static print(int32 chara) {
        Native.putchar(chara);
    }

    export static void print(char[] string) {
        for c : string {
            print(c);
        }
    }

    export static printLine(char[] string) {
        for c : string {
            print(c);
        }
        print("\n");
    }

    export static getChar() -> char {
        return (char)Native.getchar();
    }