import Kernel32;

/* TODO
    make a concept of Threading, this is just a Hack around a Windows API cause I needed a Thread
*/
export class Thread:
    thread: WThread;

    export static create(fun: (native[char]? -> int32)) -> Thread {
        wThread := WThread.create(lpStartAddress=fun, dwStackSize=0); //TODO stack size?
        return Thread(thread=wThread);
    }
