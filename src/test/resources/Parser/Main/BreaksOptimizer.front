import IO;
import Strings;

export class Main:

    export static main() {
        arr: [int64] = [int64, 1];
        arr[0] = 0;

        //breaks
        print(arr);

        //works
        printInt(arr[0]);
    }

    static print(arr: [int64]) {
        for v : arr {
            printInt(v);
        }
    }

    static printInt(l: int64) {
        IO.print(Strings.intToString(l));
    }