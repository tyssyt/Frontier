import IO;

export class Main:

    export static main() {
        arr: [int64] = [int64, 1];
        arr[0] = 0;

        //breaks
        print(arr);

        //works
        IO.printInt(arr[0]);
    }

    static print(arr: [int64]) {
        for v : arr {
            IO.printInt(v);
        }
    }