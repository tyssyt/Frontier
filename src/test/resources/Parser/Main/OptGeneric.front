import IO;

class G<T>:
    constructors;

    T t;

    get() -> T {
        return t;
    }
class O<T>:
    constructors;

    T? t;

    get() -> T? {
        return t;
    }

export class Main:

export static main() {
    IO.printInt(0);
    {
        test := new O<int64>();
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }
    {
        test := new O<int64>(2);
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }
    {
        test := new O<int64?>();
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }
    {
        test := new O<int64?>(4);
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }

    {
        test := new G<int64>(5);
        IO.printInt(test.get());
    }
    {
        test := new G<int64?>(6);
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }
    {
        test := new G<int64?>(null); //TODO this null should not be necessary, but in the current state of constructors it is, the ToDo document has a note on this
        if (test.get()) {
            IO.printInt(test.get()!);
        }
    }
    IO.printInt(8);
}