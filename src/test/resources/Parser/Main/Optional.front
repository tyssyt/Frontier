import IO;

export class Test:

    c: char;

    print() -> Test {
        IO.print(c);
        return this;
    }

    get() -> [char] {
        return "exists ";
    }

    static getLambdaIf() -> [char]? {
        return "lambdaIf";
    }

    export static main () {
        t:Test? = null;
        if t {
            t.print();
        }
        IO.print(t.print().get() : "null ");
        t = Test(c="p");
        IO.print(t.print().get() : "null ");
        t = null;
        IO.print(t.print().get() : "null ");
        t = Test(c="q");
        IO.print(t.print().get() : "null ");
        t!.print;
        if t {
            t.print();
        } else {
            return;
        }
        t.print();

        if getLambdaIf { \str ->
            IO.print(str);
        }
    }