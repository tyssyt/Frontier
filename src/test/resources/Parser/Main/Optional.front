import IO;

export class Test:

    char c;

    Test print() {
        IO.print(c);
        return this;
    }

    char[] get() {
        return "exists ";
    }

    export static void main () {
        Test? t := null;
        if t {
            t!.print();
        }
        IO.print(t.print().get() : "null ");
        t = new Test("p");
        IO.print(t.print().get() : "null ");
        t = null;
        IO.print(t.print().get() : "null ");
        t = new Test("q");
        IO.print(t.print().get() : "null ");
        if t {
            t!.print();
        }
    }