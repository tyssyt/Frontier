import IO;
class A:
    constructors;
    char[] member;
    getString() -> char[] {
        return "this is from A";
    }

class B:
    constructors;
    delegate * : A a;

class C:
    constructors;
    delegate * : B b;

export class Main:
    static testCast(A a) -> char[] {
        return a.member;
    }

    export static main () {
        C c := new C(new B(new A("castSuccessfull!")));
        IO.print(c.getString());
        IO.print(",");
        IO.print(testCast(c));
    }