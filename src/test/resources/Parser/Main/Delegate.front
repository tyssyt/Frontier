import IO;
class A:
    constructors;
    char[] member;
    getString() -> char[] {
        return "this is from A";
    }

class B:
    constructors;
    delegate * : A a;

class C:
    constructors;
    delegate * : B b;

export class Main:
    static testCast(A a) -> char[] {
        return a.member;
    }

    export static main () {
        c:C = new C(b=new B(a=new A(member="castSuccessfull!")));
        IO.print(c.getString());
        IO.print(",");
        IO.print(testCast(c));
    }