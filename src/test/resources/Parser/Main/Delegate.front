import IO;
class A:
    constructors;
    member: [char];
    getString() -> [char] {
        return "this is from A";
    }

class B:
    constructors;
    delegate * : a: A;

class C:
    constructors;
    delegate * : b: B;

export class Main:
    static testCast(a: A) -> [char] {
        return a.member;
    }

    export static main () {
        c:C = new C(b=new B(a=new A(member="castSuccessfull!")));
        IO.print(c.getString());
        IO.print(",");
        IO.print(testCast(c));
    }